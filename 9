#!/usr/bin/env python

a = 1
b = 2
c = 997

while c > b:
   while b > a:
      if (a*a + b*b) == c*c:
         print(a*b*c)
         exit()

      if b - a <= 2:
         break

      b -= 1
      a += 1

   c -= 1
   if c < 500:
      b = c - 1
      a = 1000 - c - b
   else:
      a = 1
      b = 1000 - c - a
